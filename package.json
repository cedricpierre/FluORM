{
  "name": "fluorm",
  "version": "0.0.36",
  "description": "FluORM is a fluent, model-oriented, typed REST client for TypeScript.",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "type": "module",
  "keywords": [
    "orm",
    "typescript",
    "rest",
    "api",
    "client",
    "fluent",
    "typeorm",
    "model",
    "decorators",
    "typescript-orm",
    "rest-client",
    "api-client",
    "sdk"
  ],
  "exports": {
    ".": {
      "types": "./dist/fluorm.d.ts",
      "import": "./dist/fluorm.js"
    }
  },
  "author": "Cedric Pierre",
  "email": "jirotoh@gmail.com",
  "scripts": {
    "build": "tsc && esbuild src/index.ts --bundle --format=esm --outfile=dist/fluorm.js --minify && dts-bundle-generator --config dts-bundle-generator.config.json",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cedricpierre/fluorm.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cedricpierre/fluorm/issues"
  },
  "homepage": "https://github.com/cedricpierre/fluorm#readme",
  "files": [
    "dist",
    "README.md"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@vitest/coverage-v8": "^3.1.2",
    "dts-bundle-generator": "^9.5.1",
    "esbuild": "^0.25.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.3",
    "typescript": "~5.8.0",
    "vitest": "^3.1.2"
  }
}
